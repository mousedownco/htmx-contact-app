{{ define "rows"}}
    {{ range .Data.Contacts }}
        <tr>
            <td>{{ .First }}</td>
            <td>{{ .Last }}</td>
            <td>{{ .Phone }}</td>
            <td>{{ .Email }}</td>
            <td>
                <a href="/contacts/{{ .Id }}/edit">Edit</a>
                <a href="/contacts/{{ .Id }}">View</a>
            </td>
        </tr>
    {{ end }}
{{ end }}
{{/* "part" is analagous to "content" but in the "partial" layout */}}
{{ define "part" }}
    {{ template "rows" . }}
{{ end }}